// False Arla's Dragonsbane
  CREATE ~ITM~ VERSION ~V1  ~ ~ysalbane~
  COPY ~override/ysalbane.itm~ ~override/ysalbane.itm~
    // Item Header: //
      WRITE_LONG   0x0008 6733      // Unidentified Name (strref)
      //SAY        NAME1  ~~        // Unidentified Name (strref)
      SAY          NAME2  @506      // Identified Name (strref)
      WRITE_LONG   0x0018 108       // Flags
      WRITE_SHORT  0x001c 18        // Item type
      WRITE_ASCIIE 0x0022 ~SL~ #2   // Item animation
      WRITE_SHORT  0x0026 3         // Min Strength (unused in BG1)
      WRITE_BYTE   0x002f 12        // Kit Usability 4
      WRITE_BYTE   0x0031 107       // Weapon Proficiency
      WRITE_LONG   0x0034 100       // Price
      WRITE_SHORT  0x0038 1         // Stack amount
      WRITE_ASCIIE 0x003a ~ISLNG03~ // Inventory icon (BAM)
      WRITE_ASCIIE 0x0044 ~GSLNG01~ // Ground icon (BAM)
      WRITE_LONG   0x0050 6650      // Unidentified Description (strref)
      //SAY        UNIDENTIFIED_DESC ~~ // Unidentified Description (strref)
      SAY          IDENTIFIED_DESC @507 // Identified Description (strref)
      WRITE_ASCIIE 0x0058 ~CSLNG03~ // Description icon (BAM)
      WRITE_LONG   0x0060 1         // Enchantment
    // Item Ability (Extended Header) 0 //
      LPF ADD_ITEM_HEADER
        INT_VAR
          type            = 4         // Attack type
          location        = 1         // Location
          target          = 1         // Target type
          range           = 50        // Range
          speed           = 3         // Speed
          thaco           = 1         // THAC0 bonus
          dicesize        = 6         // Dice sides
          damage          = 1         // Damage bonus
          damage_type     = 4         // Damage type
          depletion       = 1         // Charge depletion behaviour
          flags           = 4         // Flags
          projectile      = 1         // Projectile animation (projectl.ids/missile.ids)
          is_sling        = 1         // Misc. Projectile qualifier (0=No, 1=Yes)
        STR_VAR
          icon            = ~ISLNG03~ // Use icon
      END
    // Equipping Effect (Feature Block) 0 //
      LPF ADD_ITEM_EQEFFECT
        INT_VAR
             insert_point  = "-1" // The position of the feature block
             opcode        = 7    // Opcode number
             target        = 1    // Target type
             parameter1    = 101  // Parameter 1
             parameter2    = 21   // Parameter 2
             timing        = 2    // Timing mode
             probability1  = 100  // Probability 1
      END
    // Equipping Effect (Feature Block) 1 //
      LPF ADD_ITEM_EQEFFECT
        INT_VAR
             insert_point  = "-1" // The position of the feature block
             opcode        = 7    // Opcode number
             target        = 1    // Target type
             parameter1    = 236  // Parameter 1
             parameter2    = 20   // Parameter 2
             timing        = 2    // Timing mode
             probability1  = 100  // Probability 1
      END
    // Equipping Effect (Feature Block) 2 //
      LPF ADD_ITEM_EQEFFECT
        INT_VAR
             insert_point  = "-1" // The position of the feature block
             opcode        = 1    // Opcode number
             target        = 1    // Target type
             parameter1    = 1    // Parameter 1
             parameter2    = 1    // Parameter 2
             timing        = 2    // Timing mode
             probability1  = 100  // Probability 1
      END

