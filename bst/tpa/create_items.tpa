// Bone dagger
  CREATE ~ITM~ VERSION ~V1  ~ ~bsdaggd~
  COPY ~override/bsdaggd.itm~ ~override/bsdaggd.itm~
    // Item Header: //
      WRITE_LONG   0x0008 6343      // Unidentified Name (strref)
      //SAY        NAME1  ~~        // Unidentified Name (strref)
      SAY          NAME2  @518      // Identified Name (strref)
      WRITE_ASCIIE 0x0010 ~MISC56~  // BG1, BG2, BGEE: Replacement item (ITM) | PSTEE: Drop sound - played when the item is dropped in the inventory (WAV)
      WRITE_LONG   0x0018 108       // Flags
      WRITE_SHORT  0x001c 16        // Item type
      WRITE_ASCIIE 0x0022 ~DD~ #2   // Item animation
      WRITE_SHORT  0x0026 3         // Min Strength (unused in BG1)
      WRITE_BYTE   0x0031 96        // Weapon Proficiency
      WRITE_LONG   0x0034 2000      // Price
      WRITE_SHORT  0x0038 1         // Stack amount
      WRITE_ASCIIE 0x003a ~IDAGG01~ // Inventory icon (BAM)
      WRITE_ASCIIE 0x0044 ~GDAGG01~ // Ground icon (BAM)
      WRITE_LONG   0x004c 1         // Weight
      WRITE_LONG   0x0050 6720      // Unidentified Description (strref)
      //SAY        UNIDENTIFIED_DESC ~~ // Unidentified Description (strref)
      SAY          IDENTIFIED_DESC @519 // Identified Description (strref)
      WRITE_ASCIIE 0x0058 ~CDAGG01~ // Description icon (BAM)
      WRITE_LONG   0x0060 2         // Enchantment
    // Item Ability (Extended Header) 0 //
      LPF ADD_ITEM_HEADER
        INT_VAR
          type            = 1         // Attack type
          location        = 1         // Location
          target          = 1         // Target type
          thaco           = 2         // THAC0 bonus
          dicesize        = 4         // Dice sides
          dicenumber      = 1         // Dice thrown
          damage          = 2         // Damage bonus
          damage_type     = 1         // Damage type
          depletion       = 1         // Charge depletion behaviour
          flags           = 1         // Flags
          projectile      = 1         // Projectile animation (projectl.ids/missile.ids)
          backhand        = 20        // Melee animation word 2
          thrust          = 80        // Melee animation word 3
        STR_VAR
          icon            = ~IDAGG01~ // Use icon
